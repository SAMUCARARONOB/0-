ENS", Callback = function() for _, item in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do item:Clone().Parent = game.Players.LocalPlayer.Backpack end end}) Tab3:CreateToggle({Name = "FARM AUTOMÁTICO EVENTOS", CurrentValue = false, Flag = "AutoEventFarmToggle", Callback = function(enabled) while enabled do if game.Workspace:FindFirstChild("Event") then local event = game.Workspace.Event game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = event.CFrame fireproximityprompt(event.ProximityPrompt) end wait(1) end end}) Tab3:CreateButton({Name = "BYPASS VERIFICAÇÃO", Callback = function() for _, v in pairs(getgc(true)) do if typeof(v) == "function" then hookfunction(v, function() return true end) end end end}) Tab3:CreateButton({Name = "REMOVER CHECAGEM ANTI-HACK", Callback = function() for _, v in pairs(getgc(true)) do if typeof(v) == "table" and rawget(v, "AntiHack") then rawset(v, "AntiHack", function() end) end end end}) Tab3:CreateSlider({Name = "VELOCIDADE DE COLETA", Range = {1, 10}, Increment = 0.1, Suffix = "x", CurrentValue = 1, Flag = "CollectSpeedSlider", Callback = function(value) game.Players.LocalPlayer.Character.Humanoid:SetAttribute("CollectSpeed", value) end}) Tab3:CreateButton({Name = "TELEPORTAR ÁREAS SECRETAS", Callback = function() local secretAreas = { Vector3.new(1000, 100, 1000), Vector3.new(-1000, 100, -1000), Vector3.new(0, 1000, 0) } for _, area in ipairs(secretAreas) do game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(area) wait(1) end end}) Tab4:CreateToggle({Name = "ILUMINAÇÃO DO PLAYER", CurrentValue = false, Flag = "PlayerLightToggle", Callback = function(enabled) if enabled then local light = Instance.new("PointLight") light.Parent = game.Players.LocalPlayer.Character.Head light.Brightness = 10 light.Range = 20 else local light = game.Players.LocalPlayer.Character.Head:FindFirstChild("PointLight") if light then light:Destroy() end end end}) Tab4:CreateToggle({Name = "GLOW", CurrentValue = false, F