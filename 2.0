local funFeaturesTab = Window:CreateTab("Fun Features")

funFeaturesTab:CreateButton({
    Name = "Change Character Color",
    Callback = function()
        local player = game.Players.LocalPlayer
        if player and player.Character then
            for _, part in pairs(player.Character:GetChildren()) do
                if part:IsA("BasePart") then
                    part.Color = Color3.new(math.random(), math.random(), math.random())
                end
            end
        end
    end
})

funFeaturesTab:CreateToggle({
    Name = "Rainbow Effect",
    CurrentValue = false,
    Flag = "RainbowToggle",
    Callback = function(Value)
        local rainbowActive = Value
        spawn(function()
            while rainbowActive do
                local player = game.Players.LocalPlayer
                if player and player.Character then
                    for _, part in pairs(player.Character:GetChildren()) do
                        if part:IsA("BasePart") then
                            part.Color = Color3.new(math.random(), math.random(), math.random())
                        end
                    end
                end
                wait(0.5)
            end
        end)
    end
})

funFeaturesTab:CreateButton({
    Name = "Explode Character",
