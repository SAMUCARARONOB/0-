local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/SiriusSoftwareLtd/Rayfield/main/source.lua'))()

local Window = Rayfield:CreateWindow({
    Name = "Seu Script",
    LoadingTitle = "Carregando...",
    LoadingSubtitle = "Por favor, aguarde",
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nil,
       FileName = "SeuScript"
    }
})

local teleportActive = false
local teleportPosition = Vector3.new(-442100, 3.21, 66.73)

local function teleport()
    if game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(teleportPosition))
    end
end

local farmTab = Window:CreateTab("Farm")

farmTab:CreateToggle({
    Name = "FARM WIN",
    CurrentValue = false,
    Flag = "FarmToggle",
    Callback = function(Value)
        teleportActive = Value
        while teleportActive do
            teleport()
            wait(1)
        end
    end
})

farmTab:CreateToggle({
    Name = "FARM WIN FAST",
    CurrentValue = false,
    Flag = "FarmFastToggle",
    Callback = function(Value)
        teleportActive = Value
        while teleportActive do
            teleport()
            wait(0.5)
        end
    end
})

farmTab:CreateToggle({
    Name = "FARM WIN ULTRA FAST",
    CurrentValue = false,
    Flag = "FarmUltraFastToggle",
    Callback = function(Value)
        teleportActive = Value
        while teleportActive do
            teleport()
            wait(0.1)
        end
    end
})

Rayfield:LoadConfiguration()

local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
    vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    wait(1)
    vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)

local speedActive = false
local originalWalkSpeed = 16
local boostedWalkSpeed = 50

local boostsTab = Window:CreateTab("Boosts")

boostsTab:CreateToggle({
    Name = "Speed Boost",
    CurrentValue = false,
    Flag = "SpeedToggle",
    Callback = function(Value)
        speedActive = Value
        while speedActive do
            if game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
                game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = boostedWalkSpeed
            end
            wait(0.1)
        end
        if game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
            game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = originalWalkSpeed
        end
    end
})

boostsTab:CreateButton({
    Name = "Speed Boost 2",
    Callback = function()
        local player = game.Players.LocalPlayer
        if player and player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
            player.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 100
        end
    end
})

boostsTab:CreateButton({
    Name = "Super Jump",
    Callback = function()
        local player = game.Players.LocalPlayer
