local HttpService = game:GetService("HttpService")
local RANOX = loadstring(game:HttpGet("https://raw.githubusercontent.com/SAMUCARARONOB/0-/refs/heads/main/RANOX_INTEFARCE"))()

local Window = RANOX:CreateWindow({
    Title = "RANOX_HUB",
    Subtitle = "v1.0"
})

Window:CreateTab("PRINCIPAL", 4483362458)
Window:CreateTab("SETTING", 4483362458)
Window:CreateTab("CRÉDITOS", 4483362458)

Window:CreateToggle("PRINCIPAL", {
    Text = "Ativar Modo Avançado",
    Description = "Ativa recursos adicionais e avançados.",
    Default = false,
    Callback = function(isOn)
        print("Modo Avançado ativado:", isOn)
    end
})

Window:CreateToggle("PRINCIPAL", {
    Text = "Ativar Recurso X",
    Description = "Liga a função especial X do jogo.",
    Default = true,
    Callback = function(isOn)
        print("Recurso X ativado:", isOn)
    end
})

Window:CreateToggle("PRINCIPAL", {
    Text = "Habilitar Opção Y",
    Description = "Ativa a opção extra Y no ambiente.",
    Default = false,
    Callback = function(isOn)
        print("Opção Y habilitada:", isOn)
    end
})

Window:CreateLine("PRINCIPAL", Color3.fromRGB(255, 0, 0))

Window:CreateButton("SETTING", "Salvar Configurações", function()
    print("Configurações salvas!")
end)

Window:CreateLabel("SETTING", "Altere as configurações abaixo:")

Window:CreateTextBox("SETTING", "SPEED MODIFIER", function(input)
    print("Nome digitado:", input)
end)

Window:CreateDropdown("SETTING", {
    Default = "Selecione uma opção",
    Options = {"Opção 1", "Opção 2", "Opção 3"},
    Description = "Escolha uma das opções listadas abaixo.",
    Callback = function(selected)
        print("Selecionou:", selected)
    end
})

Window:CreateSlider("SETTING", {
    Text = "Volume",
    Min = 50,
    Max = 100,
    Default = 50,
    Description = "Ajuste o volume do sistema.",
    Callback = function(value)
        print("Volume ajustado para:", value)
    end
})

Window:CreateLabel("CRÉDITOS", "Feito por RANOX")
Window:CreateLabel("CRÉDITOS", "Apoie nosso trabalho!")

Window:CreateButton("CRÉDITOS", "Agradecimento", function()
    print("Obrigado pelo apoio!")
end)

Window:CreateButton("CRÉDITOS", "Visite nosso site", function()
    print("Redirecionando para o site...")
end)

Window:CreateTextBox("CRÉDITOS", "JUMP MODIFIER", function(input)
    print("Feedback recebido:", input)
end)

Window:CreateTextBox("CRÉDITOS", "MONEY MODIFIER", function(input)
    print("Email recebido:", input)
end)

Window:CreateToggle("CRÉDITOS", {
    Text = "Aceitar Termos de Uso",
    Description = "Confirma que você leu e concorda com os termos.",
    Default = false,
    Callback = function(isOn)
        print("Termos de uso aceitos:", isOn)
    end
})

Window:CreateToggle("CRÉDITOS", {
    Text = "Receber Notificações",
    Description = "Ativa ou desativa notificações do sistema.",
    Default = true,
    Callback = function(isOn)
        print("Notificações ativadas:", isOn)
    end
})

Window:CreateCheckbox("PRINCIPAL", {
    Text = "Aceitar Termos de Uso",
    Description = "Confirma sua aceitação aos termos de uso.",
    Callback = function(isChecked)
        print("Checkbox ativado:", isChecked)
    end
})

Window:CreateCheckbox("PRINCIPAL", {
    Text = "Ativar Sombras Dinâmicas",
    Description = "Habilita o efeito visual de sombras dinâmicas.",
    Callback = function(isChecked)
        print("Checkbox ativado:", isChecked)
    end
})

Window:CreateCheckbox("PRINCIPAL", {
    Text = "Mostrar FPS",
    Description = "Exibe a taxa de quadros por segundo na tela.",
    Callback = function(isChecked)
        print("Checkbox ativado:", isChecked)
    end
})

Window:CreateColorPicker("PRINCIPAL", {
    Text = "Escolha uma cor",
    Description = "Selecione uma cor para personalizar o ambiente.",
    Callback = function(color)
    end
})

Window:CreateSwitch("PRINCIPAL", {
    Text = "Modo Turbo",
    Description = "Ativa a velocidade máxima de execução.",
    State = false,
    Callback = function(state)
        print("Modo Turbo:", state)
    end
})
