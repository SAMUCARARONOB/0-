    print("Obrigado pelo apoio!")
end)

Window:CreateButton("Cr√©ditos", "Visite nosso site", function()
    print("Redirecionando para o site...")
end)

Window:CreateTextBox("Cr√©ditos", "Digite seu feedback", function(input)
    print("Feedback recebido:", input)
end)

Window:CreateTextBox("Cr√©ditos", "Digite seu email", function(input)
    print("Email recebido:", input)
end)

Window:CreateToggle("Cr√©ditos", {
    Text = "Aceitar Termos de Uso",
    Default = false,
    Callback = function(isOn)
        print("Termos de uso aceitos:", isOn)
    end
})

Window:CreateToggle("Cr√©ditos", {
    Text = "Receber Notifica√ß√µes",
    Default = true,
    Callback = function(isOn)
        print("Notifica√ß√µes ativadas:", isOn)
    end
})

Window:CreateLabel({
    Name = "Modificar Nome Visual"
})

Window:CreateTextBox({
    Placeholder = "Novo nome visual",
    Callback = function(text)
        game.Players.LocalPlayer.DisplayName = text
        showNotification("Nome visual alterado para: " .. text)
    end
})

Window:CreateSlider({
    Min = 0,
    Max = 1000,
    Default = 16,
    Color = Color3.fromRGB(0, 255, 0),
    Callback = function(value)
        applyWalkSpeed(value)
    end
})
