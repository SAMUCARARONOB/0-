local RANOX = loadstring(game:HttpGet("https://raw.githubusercontent.com/SAMUCARARONOB/0-/refs/heads/main/RANOX_INTEFARCE"))()

local Window = RANOX:CreateWindow({
    Title = "RANOX Hub"
})

-- Criando as abas
Window:CreateTab("Principal", 4483362458)
Window:CreateTab("Configura√ß√£o", 4483362458)
Window:CreateTab("Cr√©ditos", 4483362458)

-- Toggles na aba Principal
Window:CreateToggle("Principal", {
    Text = "Ativar Modo Avan√ßado",
    Default = false,
    Callback = function(isOn)
        print("Modo Avan√ßado ativado:", isOn)
    end
})

Window:CreateToggle("Principal", {
    Text = "Ativar Recurso X",
    Default = true,
    Callback = function(isOn)
        print("Recurso X ativado:", isOn)
    end
})

Window:CreateToggle("Principal", {
    Text = "Habilitar Op√ß√£o Y",
    Default = false,
    Callback = function(isOn)
        print("Op√ß√£o Y habilitada:", isOn)
    end
})

-- Aba Configura√ß√£o
Window:CreateButton("Configura√ß√£o", "Salvar Configura√ß√µes", function()
    print("Configura√ß√µes salvas!")
end)

Window:CreateLabel("Configura√ß√£o", "Altere as configura√ß√µes abaixo:")

Window:CreateTextBox("Configura√ß√£o", "Digite seu nome", function(input)
    print("Nome digitado:", input)
end)

-- ADICIONADO: Dropdown Estilizado
Window:CreateDropdown("Configura√ß√£o", {
    Default = "Selecione uma op√ß√£o",
    Options = {"Op√ß√£o 1", "Op√ß√£o 2", "Op√ß√£o 3"},
    Callback = function(selected)
        print("Selecionou:", selected)
    end
})

-- ADICIONADO: Slider Funcional
Window:CreateSlider("Configura√ß√£o", {
    Text = "Volume",
    Min = 0,
    Max = 100,
    Default = 50,
    Callback = function(value)
        print("Volume ajustado para:", value)
    end
})

-- Aba Cr√©ditos
Window:CreateLabel("Cr√©ditos", "Feito por RANOX")
