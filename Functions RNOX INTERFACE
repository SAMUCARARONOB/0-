loadstring(game:HttpGet("https://raw.githubusercontent.com/SAMUCARARONOB/0-/refs/heads/main/INTERFACE-RNOX-HUB"))() local commandBindable = game.ReplicatedStorage:FindFirstChild("CommandBindable")
if commandBindable then
    -- Configura o número de colunas no layout da interface
    commandBindable:Fire("COLUNA(1)")
    commandBindable:Fire("COLUNA(2)")
    commandBindable:Fire("COLUNA(2)")

    -- Cria botões numerados com funcionalidades
    commandBindable:Fire("BUTTON1(Abrir Dex)>255,0,255)function(loadstring(game:HttpGet('https://raw.githubusercontent.com/JustAP1ayer/ugc/main/Dex'))())")
    commandBindable:Fire("BUTTON2(Executar Dex)>0,255,255)function(loadstring(game:HttpGet('https://raw.githubusercontent.com/JustAP1ayer/ugc/main/Dex'))())")

    -- Cria toggles numerados com funcionalidades
    commandBindable:Fire("TOGGLE1(Visão Noturna)>128,128,128)function(print('Toggle 1 (Visão Noturna) ativado!'))")
    commandBindable:Fire("TOGGLE2(Escudo Ativado)>0,255,0)function(print('Toggle 2 (Escudo Ativado) ativado!'))")

    -- Cria sliders numerados com funcionalidades e cor personalizada
    commandBindable:Fire("SLIDER1(0/100)>255,0,255,function(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value)")
    commandBindable:Fire("SLIDER2(0/100)>0,255,255,function(game.Players.LocalPlayer.Character.Humanoid.JumpPower = value)")

    -- Renomeia botões específicos
    commandBindable:Fire("BUTTONN(BUTTON1, Abrir Ferramenta Dex)")
    commandBindable:Fire("BUTTONN(BUTTON2, Executar Ferramenta Dex)")

    -- Renomeia toggles específicos
    commandBindable:Fire("TOGGLEN(TOGGLE1, Ativar Modo Visão)")
    commandBindable:Fire("TOGGLEN(TOGGLE2, Barreira Protetora)")

    -- Adiciona labels aos botões e toggles numerados
    commandBindable:Fire("LABELBUTTON1>Abrir o Dex Tool>255,0,255")
    commandBindable:Fire("LABELBUTTON2>Executar o Dex Tool>0,255,255")
    commandBindable:Fire("LABELTOGGLE1>Visão Noturna Ativada>128,128,128")
    commandBindable:Fire("LABELTOGGLE2>Proteção Ligada>0,255,0")

    -- Altera o título da interface e sua cor
    commandBindable:Fire("TITLE(Painel de Controle Atualizado)")
    commandBindable:Fire("TITLECOLOR(0,128,255)")

    -- Ativa a funcionalidade de arrastar o frame principal
    commandBindable:Fire("ARRASTAR()")

    -- Aplica bordas arredondadas na interface principal
    commandBindable:Fire("BORDA()")
end
