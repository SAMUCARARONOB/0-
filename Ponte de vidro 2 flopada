() == "s" then CONTROL.B = 0 elseif k:lower() == "a" then CONTROL.L = 0 elseif k:lower() == "d" then CONTROL.R = 0 end end FLY() game:GetService("UserInputService").InputBegan:Connect(function(input) if input.KeyCode:lower() == "w" or input.KeyCode:lower() == "a" or input.KeyCode:lower() == "s" or input.KeyCode:lower() == "d" then KEYDOWN(input.KeyCode) end end) game:GetService("UserInputService").InputEnded:Connect(function(input) if input.KeyCode:lower() == "w" or input.KeyCode:lower() == "a" or input.KeyCode:lower() == "s" or input.KeyCode:lower() == "d" then KEYUP(input.KeyCode) end end) end end }) local noclipToggled = false Window:CreateToggle("FUNCTION PLAYER", { Text = "NOCLIP", Description = "Atravessar paredes", Default = false, Callback = function(state) noclipToggled = state RunService.Stepped:Connect(function() if noclipToggled and LocalPlayer.Character then for _, part in pairs(LocalPlayer.Character:GetDescendants()) do if part:IsA("BasePart") and part.CanCollide then part.CanCollide = false end end end end) end }) Window:CreateToggle("FUNCTION PLAYER", { Text = "INVISIBILIDADE TOTAL", Description = "Torna seu personagem invisível", Default = false, Callback = function(state) if state then for _, part in pairs(LocalPlayer.Character:GetDescendants()) do if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then part.Transparency = 1 end end else for _, part in pairs(LocalPlayer.Character:GetDescendants()) do if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then part.Transparency = 0 end end end end }) Window:CreateToggle("FUNCTION PLAYER", { Text = "GRAVIDADE ZERO", Description = "Desativa a gravidade", Default = false, Callback = function(state) if state then workspace.Gravity = 0 else workspace.Gravity = 196.2 end end }) Window:CreateToggle("FUNCTION PLAYER", { Text = "FLUTUAR", Description = "Flutua no ar", Default = false, Callback = function(state) if state then LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)