k = function(ativo) gigantismoAtivo = ativo end }) Window:CreateSlider("FUNCTION PLAYER", { Text = "TAMANHO DOS JOGADORES", Min = 1, Max = 10, Default = 1, Description = "Define o tamanho dos jogadores.", Callback = function(valor) tamanho = valor end }) task.spawn(function() while true do task.wait(0.00000001) if deteccaoAtiva and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then local hrp = LocalPlayer.Character.HumanoidRootPart local maisProximo = nil local menorDist = math.huge for _, p in pairs(Players:GetPlayers()) do if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then local dist = (p.Character.HumanoidRootPart.Position - hrp.Position).Magnitude if dist < menorDist and dist <= raio then menorDist = dist maisProximo = p end end end if maisProximo then local alvo = maisProximo.Character.HumanoidRootPart.Position local direcao = CFrame.new(hrp.Position, alvo) hrp.CFrame = CFrame.new(hrp.Position) * CFrame.Angles(0, direcao.LookVector:Angle(Vector3.new(0, 0, -1)), 0) end end end end) task.spawn(function() while true do task.wait(0.5) if gigantismoAtivo then for _, p in pairs(Players:GetPlayers()) do if p.Character then for _, part in pairs(p.Character:GetDescendants()) do if part:IsA("BasePart") then part.Size = Vector3.new(tamanho, tamanho, tamanho) end end end end end end end)
