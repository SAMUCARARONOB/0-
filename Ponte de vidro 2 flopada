local HttpService = game:GetService("HttpService") local RunService = game:GetService("RunService") local ReplicatedStorage = game:GetService("ReplicatedStorage") local Players = game:GetService("Players") local BrokeGlassEvent_O = ReplicatedStorage:WaitForChild("BrokeGlassEvent_O") local crateRemote = ReplicatedStorage:WaitForChild("CratesUtilities"):WaitForChild("Remotes") local freeGearEvent = ReplicatedStorage:WaitForChild("FreeGearEvent") local quintoPremio = ReplicatedStorage:WaitForChild("SpinEvents"):WaitForChild("QuintoPremio") local sextoPremio = ReplicatedStorage:WaitForChild("SpinEvents"):WaitForChild("SextoPremio") local RANOX = loadstring(game:HttpGet("https://raw.githubusercontent.com/SAMUCARARONOB/0-/refs/heads/main/RANOX_INTEFARCE"))() local Window = RANOX:CreateWindow({ Title = "RANOX HUB:      PONTE DE VIDRO DE 1M", Subtitle = "v1.0" }) Window:CreateTab("PRINCIPAL", 4483362458) local enable_breakFakeGlasses = false local enable_showPaths = false local enable_autoWin = false local enable_freeItem = false local enable_opFarm = false local enable_magicCarpet = false local function breakAllGlasses() local player = Players.LocalPlayer for _, glass in pairs(workspace:FindFirstChild("Glasses").Wrong:GetChildren()) do if glass:IsA("BasePart") and glass.Name == "glass" then BrokeGlassEvent_O:FireServer(player, glass) end end end task.spawn(function() while true do if enable_breakFakeGlasses then breakAllGlasses() end task.wait(0.5) end end) task.spawn(function() while true do if enable_showPaths then for _, glass in pairs(workspace.Glasses.Correct:GetChildren()) do if glass:IsA("BasePart") then glass.Color = Color3.fromRGB(0, 255, 0) end end for _, glass in pairs(workspace.Glasses.Wrong:GetChildren()) do if glass:IsA("BasePart") then glass.Color = Color3.fromRGB(255, 0, 0) end end end task.wait(0.5) end end) RunService.Heartbeat:Connect(function() if enable_autoWin then crateRemote.GiveReward:FireServer("10000") end end) task.spawn(function() while true do if enable_freeItem then freeGearEvent:FireServer(Players.LocalPlayer) end task.wait(0.5) end end) RunService.Heartbeat:Connect(function() if enable_opFarm then quintoPremio:FireServer(Players.LocalPlayer) end end) task.spawn(function() while true do if enable_magicCarpet then sextoPremio:FireServer(Players.LocalPlayer) end task.wait(1) end end) Window:CreateLabel("PRINCIPAL", "Farm Basic") Window:CreateToggle("PRINCIPAL", { Text = "DESTRUIR VIDROS FALSOS", Description = "Apenas Vidros Falsos serão quebrados", Default = false, Callback = function(val) enable_breakFakeGlasses = val end }) Window:CreateToggle("PRINCIPAL", { Text = "MOSTRAR CAMINHOS", Description = "Mostrará Vidros verdadeiros/falsos", Default = false, Callback = function(val) enable_showPaths = val end }) Window:CreateToggle("PRINCIPAL", { Text = "AUTO WIN", Description = "Ganhará Premium do final", Default = false, Callback = function(val) enable_autoWin = val end }) Window:CreateToggle("PRINCIPAL", { Text = "ITENS GRÁTIS", Description = "Ganhará todos os itens do Lucky block", Default = false, Callback = function(val) enable_freeItem = val end }) Window:CreateToggle("PRINCIPAL", { Text = "TAPETE MÁGICO", Description = "Ganhará Tapete Arco-Íris", Default = false, Callback = function(val) enable_magicCarpet = val end }) Window:CreateLabel("PRINCIPAL", "OP AUTO FARM MONEY") local toggle100KActive = false Window:CreateToggle("PRINCIPAL", { Text = "100K OP FARM", Description = "DINHEIRO INF VOCÊ RECEBER: 100000", Default = false, Callback = function(val) toggle100KActive = val spawn(function() while toggle100KActive do crateRemote.GiveReward:FireServer("100000") task.wait() end end) end }) local toggle200KActive = false Window:CreateToggle("PRINCIPAL", { Text = "200K OP FARM", Description = "DINHEIRO INF VOCÊ RECEBER: 200000", Default = false, Callback = function(val) toggle200KActive = val spawn(function() while toggle200KActive do crateRemote.GiveReward:FireServer("200000") task.wait() end end) end }) 