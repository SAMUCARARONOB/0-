local parts = {104,116,116,112,115,58,47,47,114,97,119,46,103,105,116,104,117,98,117,115,101,114,99,111,110,116,101,110,116,46,99,111,109,47,83,65,77,85,67,65,82,65,82,79,78,79,66,47,48,45,47,114,101,102,115,47,104,101,97,100,115,47,109,97,105,110,47,82,65,78,79,88,95,73,78,84,69,70,65,82,67,69} local function assembleURL(parts) local url = "" for _, part in ipairs(parts) do url = url .. string.char(part) end return url end local RANOX = loadstring(game:HttpGet(assembleURL(parts)))() local Players = game:GetService("Players") local RunService = game:GetService("RunService") local LocalPlayer = Players.LocalPlayer local Window = RANOX:CreateWindow({ Title = "Ranox Hub:     PONTE DE VIDRO JOGO DE LULA", Subtitle = "v1.0" }) Window:CreateTab("MAIN", 4483362458) Window:CreateLabel("MAIN", "AUTO FARM") local destroyGlass = false Window:CreateToggle("MAIN", { Text = "DESTRUIR VIDROS", Description = "Destruir Todos os Vidros Falsos Automaticamente.", Default = false, Callback = function(state) destroyGlass = state task.spawn(function() while destroyGlass do for _, v in ipairs(workspace:GetDescendants()) do if v.Name == "Vidrio roto" and v:IsA("BasePart") then firetouchinterest(LocalPlayer.Character.HumanoidRootPart, v, 0) firetouchinterest(LocalPlayer.Character.HumanoidRootPart, v, 1) end end task.wait(0.25) end end) end }) local autoFarmMoney = false Window:CreateToggle("MAIN", { Text = "AUTO FARM MONEY", Description = "Coleta automaticamente o prêmio do final do mapa.", Default = false, Callback = function(state) autoFarmMoney = state RunService.Heartbeat:Connect(function() if autoFarmMoney then local part = workspace:FindFirstChild("PremioMAXTSAINTS", true) if part and part:IsA("BasePart") then firetouchinterest(LocalPlayer.Character.HumanoidRootPart, part, 0) firetouchinterest(LocalPlayer.Character.HumanoidRootPart, part, 1) end end end) end }) local vidColor = Color3.fromRGB(255, 0, 0) local revealActive = false Window:CreateCheckbox("MAIN", { Text = "REVELA VIDROS", Description = "Revela cor de Vidros Verdadeiros/Falsos.", Callback = function(state) revealActive = state task.spawn(function() while revealActive do for _, obj in ipairs(workspace:GetDescendants()) do if obj:IsA("BasePart") then if obj.Name == "Vidrio templado" then obj.Color = Color3.fromRGB(0, 255, 0) elseif obj.Name == "Vidrio roto" then obj.Color = vidColor end end end task.wait(1) end end) end }) Window:CreateColorPicker("MAIN", { Text = "COR VIDROS QUEBRÁVEIS", Callback = function(color) vidColor = color if revealActive then for _, obj in ipairs(workspace:GetDescendants()) do if obj:IsA("BasePart") and obj.Name == "Vidrio roto" then obj.Color = vidColor end end end end }) Window:CreateSlider("MAIN", { Text = "Velocidade", Min = 16, Max = 10000, Default = 16, Callback = function(value) if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then LocalPlayer.Character.Humanoid.WalkSpeed = value end end }) Window:CreateSlider("MAIN", { Text = "Pulo", Min = 16, Max = 10000, Default = 50, Callback = function(value) if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then LocalPlayer.Character.Humanoid.JumpPower = value end end })
